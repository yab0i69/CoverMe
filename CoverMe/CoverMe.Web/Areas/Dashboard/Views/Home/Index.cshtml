@model CoverMe.Web.Areas.Dashboard.Models.HomeViewModels.HomeViewModel

@{
	ViewData["Title"] = "Dashboard";
}

<div class="row">
	<!-- Page title -->
	<div class="page-header d-print-none">
		<div class="row align-items-center">
			<div class="col">
				<!-- Page pre-title -->
				<div class="page-pretitle">
					Overview
				</div>
				<h2 class="page-title">
					@Model.User.UserName.Trim()'s Dashboard
				</h2>
			</div>
			<!-- Page title actions -->
			<div class="col-auto ms-auto d-print-none">
				<div class="btn-list">
                    <a asp-area="Dashboard" asp-controller="Home" asp-action="Test" class="btn btn-white">
						<span class="d-none d-sm-inline">
								View Test Page
						</span>
					</a>
					<span class="d-none d-sm-inline">
						<a href="#" class="btn btn-white">
							Join Business
						</a>
					</span>
					<a href="#" class="btn btn-primary d-sm-none btn-icon" data-bs-toggle="modal" data-bs-target="#modal-report" aria-label="Create new report">
						<!-- Download SVG icon from http://tabler-icons.io/i/plus -->
						<svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
							<path stroke="none" d="M0 0h24v24H0z" fill="none"></path><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line>
						</svg>
					</a>
				</div>
			</div>
		</div>
	</div>
</div>


<!-- END SAMPLE TABLER DASHBOARD -->